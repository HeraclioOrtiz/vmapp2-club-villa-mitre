# Environment Configuration for Club Villa Mitre App

# API Configuration
# For development with local Laravel backend
API_BASE_URL=http://localhost:8000/api

# For production (replace with your actual production URL)
# API_BASE_URL=https://your-production-api.com/api

# Development Configuration
# Set to 'true' to use Mirage.js mock server for development
# Set to 'false' to use real Laravel backend API
USE_MIRAGE_SERVER=false

# Other environment variables
APP_VERSION=1.0.0
